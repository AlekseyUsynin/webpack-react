{
	"name": "webpack-react",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --env env=prod",
		"start": "webpack serve --config webpack/webpack.config.js --env env=dev",
		"stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
		"lint": "eslint --fix",
		"format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path ./.prettierignore",
		"check": "npm run stylelint:fix && npm run lint && npm run format",
		"commit": "npm run check && cz",
		"prepare": "husky"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*.{css,scss}": "npm run stylelint:fix",
		"*.{js,jsx,ts,tsx}": "npm run lint",
		"*.{js,jsx,ts,tsx,json,css,scss,md}": "npm run format"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
		"@svgr/webpack": "^8.1.0",
		"@types/node": "^24.2.1",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@typescript-eslint/eslint-plugin": "^8.39.0",
		"@typescript-eslint/parser": "^8.39.0",
		"autoprefixer": "^10.4.21",
		"commitizen": "^4.3.1",
		"cross-env": "^10.0.0",
		"css-loader": "^7.1.2",
		"cssnano": "^7.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"html-webpack-plugin": "^5.6.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"mini-css-extract-plugin": "^2.9.3",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.6.2",
		"react-refresh": "^0.17.0",
		"sass": "^1.90.0",
		"sass-loader": "^16.0.5",
		"standard-version": "^9.5.0",
		"style-loader": "^4.0.0",
		"stylelint": "^16.23.1",
		"stylelint-config-recommended": "^17.0.0",
		"stylelint-config-standard": "^39.0.0",
		"stylelint-order": "^7.0.0",
		"ts-loader": "^9.5.2",
		"typescript": "^5.9.2",
		"url-loader": "^4.1.1",
		"webpack": "^5.101.0",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^4.15.1",
		"webpack-merge": "^6.0.1"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	}
}
